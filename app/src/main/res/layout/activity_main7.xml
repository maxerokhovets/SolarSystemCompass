<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.compass.Main7Activity" android:background="@color/common_google_signin_btn_text_dark_focused">

    <TextView
            android:layout_width="250dp"
            android:layout_height="100dp"
            android:id="@+id/latTextView"
            android:textSize="30sp" android:textColor="#FFFFFF"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="338dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="293dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="145dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="250dp"
            android:layout_height="100dp"
            android:id="@+id/lonTextView"
            android:textColor="#FFFFFF" android:textSize="30sp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="423dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="145dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="208dp"/>
    <ImageView
            android:layout_width="242dp"
            android:layout_height="376dp" app:srcCompat="@drawable/location"
            android:id="@+id/imageView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="37dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="318dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="84dp" android:layout_marginEnd="85dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>